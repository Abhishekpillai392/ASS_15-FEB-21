=>

//ass_2.class

package pack1;

import java.util.Stack;

public class ass_2 {

	public static void main(String[] args) {

		Stack<String> st= new Stack<String>();
		
		st.push("ABHISHEK");
		st.push("ABHINAV");
		st.push("ABHILASH");
		st.push("RONAUK");
		st.push("EDEN");
		
		System.out.println(st);
	}

}

